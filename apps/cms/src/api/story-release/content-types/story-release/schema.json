{
  "kind": "collectionType",
  "collectionName": "story_releases",
  "info": {
    "singularName": "story-release",
    "pluralName": "story-releases",
    "displayName": "Story Release",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "story": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::story.story",
      "inversedBy": "releases"
    },
    "type": {
      "type": "enumeration",
      "enum": ["movie", "series", "game", "short", "book"]
    },
    "releaseDate": {
      "type": "date"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "releases"
    },
    "crosscheck": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::crosscheck.crosscheck",
      "mappedBy": "story_release"
    },
    "copy": {
      "type": "component",
      "repeatable": false,
      "component": "shared.content"
    },
    "findReplaceReject": {
      "type": "component",
      "repeatable": true,
      "component": "jolly-roger.find-replace-reject"
    }
  }
}
