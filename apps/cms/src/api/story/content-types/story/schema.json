{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "singularName": "story",
    "pluralName": "stories",
    "displayName": "Story",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "studio": {
      "type": "enumeration",
      "enum": ["disney", "pixar", "marvel", "lucasfilm"]
    },
    "shortDescription": {
      "type": "string"
    },
    "fullDescription": {
      "type": "text"
    },
    "cast": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::story-cast.story-cast",
      "inversedBy": "stories"
    },
    "releases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::story-release.story-release",
      "mappedBy": "story"
    },
    "fandom": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::fandom.fandom",
      "inversedBy": "stories"
    },
    "metas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::meta.meta",
      "inversedBy": "stories"
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "mappedBy": "stories"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "stories"
    },
    "crosscheck": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::crosscheck.crosscheck",
      "mappedBy": "story"
    },
    "copy": {
      "type": "component",
      "repeatable": false,
      "component": "shared.content"
    },
    "findReplaceReject": {
      "type": "component",
      "repeatable": true,
      "component": "jolly-roger.find-replace-reject"
    }
  }
}
