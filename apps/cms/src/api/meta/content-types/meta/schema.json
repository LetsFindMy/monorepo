      "target": "api::story.story",
      "mappedBy": "metas"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "inversedBy": "metas"
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "mappedBy": "metas"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "metas"
    },
    "crosscheck": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::crosscheck.crosscheck",
      "mappedBy": "metas"
    },
    "copy": {
      "type": "component",
      "repeatable": false,
      "component": "shared.content"
    },
    "findReplaceReject": {
      "type": "component",
