    },
    "crosscheck": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::crosscheck.crosscheck",
      "mappedBy": "event"
    },
    "copy": {
      "type": "component",
      "repeatable": false,
      "component": "shared.content"
