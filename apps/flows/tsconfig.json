{
  "extends": [
    "@repo/typescript-config/nextjs.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#/app/*": [
        "./app/*"
      ],
      "#/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "next.config.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src/lib/prisma/generated/zod/index.ts"
  ]
}