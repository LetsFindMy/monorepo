{
  "name": "flows",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "prebuild": "pnpm prisma:generate",
    "build": "next build",
    "build:turbo": "npx turbo build",
    "dev": "next dev  -p 3003 --turbo",
    "doppler": "doppler secrets download --no-file --format env > .env",
    "postinstall": "prisma generate --schema=./src/lib/prisma/schema.prisma",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma format && prisma generate",
    "prisma:introspect": "prisma introspect",
    "prisma:push": "prisma format && prisma db push && pnpm build",
    "prisma:studio": "prisma studio",
    "prisma:validate": "prisma validate",
    "start": "next start",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
    "tsc": "npx tsc --noEmit"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^17.0.0",
    "@mantine/core": "^7.16.0",
    "@mantine/form": "^7.16.0",
    "@mantine/hooks": "^7.16.0",
    "@mantine/modals": "^7.16.0",
    "@mantine/notifications": "^7.16.0",
    "@prisma/client": "^6.2.1",
    "@tabler/icons-react": "^3.28.1",
    "@xyflow/react": "^12.4.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^11.18.0",
    "next": "^15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.5.1",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^6.2.1",
    "sass": "1.68.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "zod-prisma-types": "^3.2.1"
  },
  "prisma": {
    "schema": "./src/lib/prisma/schema.prisma"
  }
